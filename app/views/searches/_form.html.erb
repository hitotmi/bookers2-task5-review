<% if signe_in %>
  <div class="search">
    <%= form_with url: search_path, local: true,method: :get do |f| %>
      <%= f.text_field :word %>
      <%= select :range,options_for_select([['User'], ['Book']]) %>
      <%= select :search,optons_for_select([["完全一致", "perfect_match"],["前方一致", "forword_match"],["後方一致", "backword_match"],["部分一致", "partial_match"]]) %>
      <%= f.submit "検索",class: "btn btn-primary" %>
    <% end %>
  </div>
<% end %>